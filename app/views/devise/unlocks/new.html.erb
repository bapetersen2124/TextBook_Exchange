<div class="form-outline carousel-image-background form-background poppins">

<h2 class="form-header">Resend unlock instructions</h2>

<%= form_for(resource, as: resource_name, url: unlock_path(resource_name), html: { method: :post }) do |f| %>
  <%= devise_error_messages! %>

<div class="row">
  <div class="col-md-12 form-group">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, class:"form-control" %>
  </div>
</div>

  <div class="actions btn btn-primary">
    <%= f.submit "Resend unlock instructions", class:"submit-styling" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
</div>
